{
    "name": "vxm/yii2-mobile-first",
    "description": "Support implementing mobile first principle for Yii2 application.",
    "type": "yii2-extension",
    "keywords": ["yii2","extension","mobile-first"],
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Vuong Minh",
            "email": "vuongxuongminh@gmail.com"
        }
    ],
    "require": {
        "php": "^7.1",
        "yiisoft/yii2": "~2.0.13",
        "mobiledetect/mobiledetectlib": "^2.8"
    },
    "autoload": {
        "psr-4": {
            "vxm\\mobileFirst\\": "/src"
        }
    },
    "repositories": [
        {
            "type": "composer",
